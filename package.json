{
  "name": "vue-dynamix",
  "description": "A vue Library for custom elements and components",
  "publishConfig": {
    "access": "public"
  },
  "author": "Sohype Khaled <sohype.mop@gmail.com>",
  "license": "MIT",
  "private": false,
  "version": "0.0.4",
  "type": "module",
  "main": "dist/lib/dynamix.umd.js",
  "module": "dist/lib/dynamix.es.js",
  "types": "dist/lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/dynamix.es.js",
      "require": "./dist/lib/dynamix.umd.js"
    },
    "./index.css": "./dist/lib/index.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/lib"
  ],
  "scripts": {
    "dev": "vite",
    "dev:lib": "vite --mode library",
    "dev:ce": "vite --mode custom-element",
    "clean": "rm -rf dist",
    "build:lib": "vue-tsc -b && vite build --mode library",
    "build:ce": "vue-tsc -b && vite build --mode custom-element",
    "build": "yarn clean && yarn build:lib && yarn build:ce",
    "prepare:publish": "rm -rf publish && mkdir publish && cp -r dist/lib/* publish/ && cp package.json publish/",
    "pack:lib": "yarn build:lib && yarn prepare:publish && cd publish && yarn pack",
    "preview": "vite preview"
  },
  "dependencies": {},
  "peerDependencies": {
    "@tailwindcss/vite": "^4.0.13",
    "axios": "^1.8.2",
    "flowbite": "^3.1.2",
    "tailwindcss": "^4.0.13",
    "vue": "^3.5.13",
    "vuedraggable": "^4.1.0",
    "@iconify/vue": "^4.3.0",
    "pinia": "^3.0.1",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@iconify/vue": "^4.3.0",
    "@tailwindcss/vite": "^4.0.13",
    "@types/axios": "^0.14.4",
    "@types/node": "^22.13.10",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "axios": "^1.8.2",
    "dts-bundle-generator": "^9.5.1",
    "flowbite": "^3.1.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "sass": "^1.85.1",
    "tailwindcss": "^4.0.13",
    "typescript": "~5.7.2",
    "pinia": "^3.0.1",
    "yup": "^1.6.1",
    "vite": "^6.2.0",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.5.3",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.4",
    "vuedraggable": "^4.1.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
